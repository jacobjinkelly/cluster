{
  "data": "mnist",
  "n_iters": 10000,
  "batch_size": [100],
  "save_best": true,
  "lr": [1e-2, 1e-3],
  "hidden_units": {
    "key": "arch",
    "dist": "lin",
    "start": 3,
    "stop": 6,
    "num": 4,
    "dtype": "int"
  },
  "layers": {
    "key": "arch",
    "dist": "lin",
    "start": 10,
    "stop": 7,
    "num": 4,
    "dtype": "int"
  },
  "lr_decay_factor": {
    "dist": "log",
    "start": -1,
    "stop": -3,
    "num": 3
  },
  "early_stopping": [true, false],
  "batch_norm": {
    "key": "no_dropout_with_bn",
    "values": [true, false]
  },
  "dropout": {
    "key": "no_dropout_with_bn",
    "values": [0, 0.5]
  }
}
